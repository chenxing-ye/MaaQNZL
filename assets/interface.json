{
    "name": "MaaQNZL",
    "url": "https://github.com/chenxing-ye/MaaQNZL",
    "custom_title": "千年之旅小助手",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "-u",
            "{PROJECT_DIR}/../agent/main.py"
        ]
    },
    "task": [
        {
            "name": "启动游戏",
            "entry": "StartUp"
        },
        {
            "name": "神谕司",
            "entry": "Harvest"
        },
        {
            "name": "每日骰子",
            "entry": "Daily"
        },
        {
            "name": "商店",
            "entry": "Shop",
            "pipeline_override": {
                "ItemBuyConfirm": {
                    "next": [
                        "ItemBuyFail",
                        "ShopDailyHarvestBuy"
                    ]
                }
            }
        },
        {
            "name": "限时活动",
            "entry": "ActivityDungeon",
            "option": [
                "清体力",
                "当前活动"
            ]
        },
        {
            "name": "刷本-清体力",
            "entry": "Farm",
            "option": [
                "副本"
            ]
        },
        {
            "name": "公会每日",
            "entry": "GuildDaily",
            "pipeline_override": {
                "Battling": {
                    "interrupt": [
                        "CloseAutobattle",
                        "Skipbattle"
                    ]
                }
            },
            "option": [
                "公会首领出战队伍"
            ]
        },
        {
            "name": "出击-助战",
            "entry": "AssistInFighting",
            "repeatable": true,
            "repeat_count": 3,
            "doc": "助战会默认使用一队进行，导致一队角色混乱，其他玩法需要打高分，建议手动设置好二队及其自动轴"
        },
        {
            "name": "每日任务",
            "entry": "Task"
        },
        {
            "name": "深渊之域",
            "entry": "Abyss",
            "pipeline_override": {
                "ClickBattleConfirm": {
                    "interrupt": [
                        "CloseAutobattle",
                        "Skipbattle",
                        "AbyssNewLevel"
                    ]
                }
            },
            "option": [
                "出战队伍"
            ]
        },
        {
            "name": "怪物列阵",
            "entry": "ColumnMatrix"
        },
        {
            "name": "黯影地牢",
            "entry": "Roguelike",
            "option": [
                "出战队伍"
            ]
        },
        {
            "name": "回忆深潜",
            "entry": "Tower",
            "option": [
                "出战队伍",
                "塔"
            ]
        },
        {
            "name": "结束游戏",
            "entry": "CloseQNZL"
        },
        {
            "name": "钓鱼",
            "entry": "StartFishing",
            "doc": "需要手动选择好钓点后在钓鱼主界面执行，鱼标记点击不准确，建议高等级之后使用(本人钓鱼七级以上使用没问题)"
        }
        ,
        {
            "name": "主线自动推图",
            "entry": "AutoStory",
            "option": [
                "出战队伍",
                "自动吃体力"
            ],
            "doc": "要推的图的主界面执行"
        }
    ],
